{
  "name": "@metagen/core",
  "version": "2.1.3",
  "description": "File tree template engine",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akondratsky/metagen.git"
  },
  "keywords": [
    "generator",
    "templates",
    "files",
    "tree"
  ],
  "type": "module",
  "author": "Aleksandr Kondratskii",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akondratsky/metagen/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "bun test",
    "lint": "bun run lint:tsc && eslint src",
    "lint:tsc": "tsc --project tsconfig.test.json --noEmit && tsc --project tsconfig.lib.json --noEmit",
    "build": "rimraf ./dist/* && bun run build:tsc && bun dist.ts",
    "build:tsc": "tsc --project tsconfig.lib.json",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/akondratsky/metagen#readme",
  "devDependencies": {
    "@commitlint/cli": "17.8.0",
    "@commitlint/config-conventional": "17.8.0",
    "@stylistic/eslint-plugin": "1.0.0",
    "@typescript-eslint/eslint-plugin": "7.4.0",
    "@typescript-eslint/parser": "7.4.0",
    "bun-types": "1.0.8",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.53.0",
    "eslint-plugin-require-extensions": "0.1.3",
    "husky": "^8.0.0",
    "rimraf": "5.0.5",
    "typescript": "5.4.3"
  },
  "dependencies": {
    "@technically/lodash": "4.17.0",
    "commander": "11.1.0",
    "handlebars": "4.7.8",
    "istextorbinary": "9.5.0"
  }
}
